build

    cd gcc
    mkdir -p libstdc++-v3-build
    cd libstdc++-v3-build
    CFLAGS="-g -O0" CXXFLAGS="-g -O0" ../libstdc++-v3/configure prefix=/usr
    make -j8
    DESTDIR=`pwd`/out make install

re-build

    make clean
    make -j8
    DESTDIR=`pwd`/out make install

